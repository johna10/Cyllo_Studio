                            <span class="checkbox-indicator">
                                <svg width="12px" height="9px" viewbox="0 0 12 9">
                                    <polyline points="1 5 4 8 11 1"></polyline>
                                </svg>
                            </span>
                            <span class="label-text">Can Create</span>
                        </label>
                    </div>
                    <div class="checkbox-item">
                        <input class="checkbox-input ViewCheck" type="checkbox" id="canEdit" name="edit" style="display: none;" t-model="props.mode.edit" t-on-click="(ev)=> this.props.handleView('edit',!props.mode.edit)"/>
