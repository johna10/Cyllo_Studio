<templates xml:space="preserve">

    <t t-inherit="cyllo_web.Systray" t-inherit-mode="extension">
        <xpath expr="//t[@t-foreach='state.alwaysItems']" position="before">
            <div class="cy-right-header-menugroup-item">
                <div class="new_icon">
                    <a class="toggle-icon" type="button" t-on-click="_onClick" tabindex="0" aria-expanded="false">
                        <i id="cyllo_studio" class="oi oi-studio fa-lg" role="img" aria-label="Studio"/>
                    </a>
                </div>
            </div>
        </xpath>
    </t>
</templates>