<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
	<t t-name="cyllo_studio.UndoRedo" owl="1">
        	<div t-att-class="!state.undo ? 'cy-undo-disable' : ''" t-att-style="!state.undo ? 'pointer-events: none;':''">
						<button class="btn cy-arrow-btn" t-on-click="undoChange">
							<i class="ri-arrow-go-back-line"/>
						</button>
					</div>
					<div t-att-class="!state.redo ? 'cy-undo-disable' : ''" t-att-style="!state.redo ? 'pointer-events: none;':''">
						<button class="btn cy-arrow-btn" t-on-click="redoChange">
							<i class="ri-arrow-go-forward-line"/>
						</button>
					</div>
    </t>
</templates>